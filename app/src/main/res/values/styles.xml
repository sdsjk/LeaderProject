    <!--footer end-->

    <!--列表-->

    <style name="listViewIconRight" parent="text">
        <item name="android:textColor">@color/textLighestGrey</item>
        <item name="android:textSize">14dp</item>
        <item name="android:text">@string/icon_arrow_right</item>
        <item name="android:layout_marginLeft">2dp</item>
    </style>

    <style name="listViewWrapper">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:showDividers">middle</item>
        <item name="android:paddingTop">1px</item>
        <item name="android:paddingBottom">1px</item>
        <!--<item name="android:background">@drawable/shape_border_vertical_transparent</item>-->
