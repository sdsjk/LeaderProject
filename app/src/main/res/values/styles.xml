    <style name="listViewItem">
        <item name="android:background">@drawable/selector_list_item_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:paddingLeft">@dimen/medium_margin</item>
        <item name="android:paddingRight">@dimen/medium_margin</item>
        <item name="android:textSize">@dimen/medium_text</item>
    </style>

    <style name="listViewItemXiaoxi">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:paddingLeft">@dimen/medium_margin</item>
        <item name="android:paddingRight">@dimen/medium_margin</item>
        <item name="android:textSize">@dimen/medium_text</item>
    </style>
