        <item name="android:showDividers">middle</item>
    </style>
    <style name="formLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:layout_marginLeft">@dimen/medium_margin</item>
        <item name="android:layout_marginRight">@dimen/medium_margin</item>
    </style>

    <style name="formLine.single">
        <item name="android:paddingLeft">@dimen/medium_margin</item>
        <item name="android:paddingRight">@dimen/medium_margin</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:background">@color/colorWhite</item>
    </style>

    <style name="formLeftText" parent="text">
        <item name="android:textColor">@color/textGrey</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="formRightText" parent="text">
        <item name="android:minWidth">10dp</item>
        <item name="android:maxWidth">220dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColorHint">@color/textHintColor</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center|left</item>
        <item name="android:textColor">@color/textDark</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="formRightText.withArrow">
