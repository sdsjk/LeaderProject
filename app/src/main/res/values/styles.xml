        <item name="android:clickable">true</item>
    </style>

    <style name="listViewText.right">
        <item name="android:layout_marginRight">@dimen/normal_margin</item>
        <item name="android:textColor">@color/textDark</item>
        <item name="android:textColorHint">@color/textLighestGrey</item>
        <item name="android:layout_weight">0</item>
    </style>

    <style name="listViewItem.paddingRight">
        <item name="android:paddingRight">@dimen/medium_big_margin</item>
    </style>
    <!--列表 end-->

    <!--弹框-->
    <style name="dialogTitle" parent="text">
        <item name="android:textSize">@dimen/medium_text</item>
        <item name="android:gravity">center</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginLeft">@dimen/big_margin</item>
        <item name="android:layout_marginRight">@dimen/big_margin</item>
        <item name="android:layout_marginTop">22dp</item>
        <item name="android:layout_marginBottom">@dimen/small_margin</item>
    </style>
    <style name="dialogSubTitle" parent="text">
