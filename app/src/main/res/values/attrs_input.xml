<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UInput">
        <attr name="hint" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="maxLength" format="integer"></attr>
        <attr name="maxWidth" format="integer"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="verifytextColor" format="color"></attr>
        <attr name="indexTextColor" format="color"></attr>
        <attr name="textColorHint" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="TextAreaInput">
        <!--输入框的提示-->
        <attr name="textarea_hint" format="string" />
        <!--输入的字数-->
        <attr name="word_size" format="integer" />
        <!--标题-->
        <attr name="text_title" format="string" />
        <!--最低行数-->
        <attr name="min_line" format="integer" />
    </declare-styleable>

    <declare-styleable name="process">
        <!--流程数组，以逗号隔开，有默认-->
        <attr name="process" format="string" />
        <!--当前-->
        <attr name="current" format="integer" />
    </declare-styleable>

    <declare-styleable name="rateline">
        <!--流程数组，以逗号隔开，有默认-->
        <attr name="name" format="string" />
    </declare-styleable>

    <!--数量选择-->
    <declare-styleable name="CountInput">
        <!--输入框的提示-->
        <attr name="title" format="string" />
        <!--输入的字数-->
        <attr name="init_count" format="integer" />
    </declare-styleable>

    <!--地址信息-->
    <declare-styleable name="AddressInfo">
        <!--输入框的提示-->
        <attr name="has_title" format="boolean" />
        <attr name="user_info_title" format="string" />
        <attr name="address_info_title" format="string" />
    </declare-styleable>

    <declare-styleable name="FixHeightRelativeLayout">
        <attr name="height_times" format="string" />
    </declare-styleable>

</resources>